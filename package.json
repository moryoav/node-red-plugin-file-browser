{
  "name": "@moryoav/node-red-plugin-file-browser",
  "version": "0.2.2",
  "description": "Node-RED editor plugin: Files sidebar with folder tree, text editor, create/save files; base folder is configurable from the UI and persisted under userDir.",
  "license": "MIT",
  "author": "Yoav (moryoav)",
  "homepage": "https://github.com/moryoav/node-red-plugin-file-browser#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/moryoav/node-red-plugin-file-browser.git"
  },
  "bugs": {
    "url": "https://github.com/moryoav/node-red-plugin-file-browser/issues"
  },
  "keywords": [
    "node-red",
    "node-red-plugin",
    "node-red-editor-plugin",
    "file-browser",
    "editor",
    "monaco"
  ],
  "node-red": {
    "version": ">=3.0.0",
    "plugins": {
      "file-browser": "editor/file-browser.html"
    },
    "nodes": {
      "file-browser-admin": "runtime/admin.js"
    }
  },
  "files": [
    "editor",
    "runtime",
    "examples",
    "README.md",
    "LICENSE"
